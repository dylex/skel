local s
if [[ $# -ge 2 && $1 = -j ]] ; then
	shift
	[[ $# -eq 1 ]] && set -- "$@" "$@"
	set -- "$@" \; set status off \; link-window -k -s main:$1 -t 0
fi
s=${1:-main} ; [[ $# -ge 1 ]] && shift
if tmux has-session -t $s ; then
	tmux attach-session -t $s
else
	tmux new-session -s $s -n "$1" "$@" \; set-option -t $s default-path $PWD
fi
