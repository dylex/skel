[core]
	pager = less -FX
	excludesfile = ~/.gitignore
[push]
	default = current
[rerere]
	enabled = false
[alias]
	st = status
	ci = commit -v
	ch = cherry -v
	cia = commit -va
	amend = commit -v --amend
	co = checkout -m
	logs = log --stat
	logp = log -p
	diffs = diff --stat
	b = branch -v
	ba = branch -av
	cat = show --format=raw
	ap = cherry-pick -n
	up = remote update --prune
[color]
	ui = auto
[merge]
	tool = vimdiff
[log]
	date = local
